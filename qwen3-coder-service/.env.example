# Qwen3-Coder-30B Service Configuration
# Copy to .env and fill in your values

# =============================================================================
# vLLM Model Configuration
# =============================================================================
MODEL_ID=Qwen/Qwen3-Coder-30B-A3B-Instruct-FP8
SERVED_MODEL_NAME=qwen3-coder-30b-a3b-fp8
TP_SIZE=1
GPU_MEMORY_UTILIZATION=0.92
NUM_CONCURRENT_REQUESTS=16
MAX_BATCHED_TOKENS=32768
MAX_MODEL_LEN=32768

# Local API key for vLLM (used internally between connector and vLLM)
API_KEY=your-local-api-key-here

# vLLM port (default: 8000)
VLLM_PORT=8000

# =============================================================================
# RemoteLLMconnector Configuration
# =============================================================================
# Your broker's WebSocket URL (e.g., wss://your-broker.example.com/ws/connector)
BROKER_WS_URL=wss://your-broker-url/ws/connector

# Connector authentication token (from your broker configuration)
CONNECTOR_TOKEN=your-connector-token-here
